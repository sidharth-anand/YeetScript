[KEYWORDS]
    STATE_INITIAL - KEYWORDS - STATE_INITIAL emit KEYWORD

[IDENTIFIERS]
    STATE_INITIAL - CHARACTER - STATE_CHARACTER

    STATE_CHARACTER - CHARACTER - STATE_CHARACTER
    STATE_CHARACTER - DELIMITER - STATE_INITIAL emit IDENTIFIER/DELIMITER
    STATE_CHARACTER -   - STATE_INITIAL emit IDENTIFIER/WHITESPACE

[INT LITERAL]
    STATE_INITIAL - DIGIT - STATE_DIGIT

    STATE_DIGIT - DIGIT - STATE_DIGIT
    STATE_DIGIT - DELIMITER - STATE_INITIAL emit INTEGER/DELIMITER
    STATE_DIGIT -   - STATE_INITIAL emit IDENTIFIER/WHITESPACE

[WHITESPACE]
    STATE_INITIAL -   - STATE_INITIAL emit WHITESPACE

[DELIMITERS]
    STATE_INITIAL - DELIMITER - STATE_INITIAL emit DELIMITER

[SINGLE CHARACTER COMPARISON]
    STATE_INITIAL - COMPARISON - STATE_INITIAL emit COMPARISON